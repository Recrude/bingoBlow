  "bugs": {
    "url": "https://github.com/mihailgaberov/bingo-blower/issues"
  },
  "homepage": "https://github.com/mihailgaberov/bingo-blower#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "^6.23.0",
    "colors": "^1.1.2",
    "eventsource-polyfill": "0.9.6",
    "express": "^4.15.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "path": "^0.12.7",
